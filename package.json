{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "prebuild": "node scripts/generate-snap-urls.js",
    "postbuild": "react-snap",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "vite preview --host 0.0.0.0 --port ${PORT:-5173}",
    "docker:dev": "docker-compose up --build",
    "docker:backend": "cd backend && npm run dev"
  },
  "reactSnap": {
    "source": "dist",
    "minifyHtml": {
      "collapseWhitespace": false,
      "removeComments": false
    },
    "crawl": true,
    "include": [
      "/",
      "/converters",
      "/compress",
      "/samples",
      "/viewers"
    ],
    "skipThirdPartyRequests": true,
    "cacheAjaxRequests": false,
    "http2PushManifest": false,
    "puppeteerArgs": ["--no-sandbox", "--disable-setuid-sandbox"]
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4",
    "@types/emscripten": "^1.41.2",
    "@types/three": "^0.163.0",
    "exif-js": "^2.3.0",
    "heic2any": "^0.0.4",
    "html2canvas": "^1.4.1",
    "i18next": "^23.7.0",
    "i18next-browser-languagedetector": "^7.2.0",
    "jspdf": "^3.0.2",
    "jszip": "^3.10.1",
    "libraw.js": "^3.0.0",
    "lucide-react": "^0.344.0",
    "pdf-lib": "^1.17.1",
    "pdfjs-dist": "^4.8.69",
    "piexifjs": "^1.0.6",
    "raw-loader": "^4.0.2",
    "raw.js": "^0.6.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.8",
    "react-helmet-async": "^2.0.4",
    "react-i18next": "^14.0.0",
    "react-router-dom": "^6.28.0",
    "tesseract.js": "^6.0.1",
    "three": "^0.163.0",
    "tiff.js": "^1.0.0",
    "utif2": "^4.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/jszip": "^3.4.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
