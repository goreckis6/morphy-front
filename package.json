{
  "name": "formipeek-qwik",
  "version": "0.0.1",
  "description": "FormiPeek - File Converter with Qwik + React",
  "type": "module",
  "scripts": {
    "dev": "vite dev --mode ssr",
    "build": "qwik build && vite build --ssr src/entry.ssr.tsx",
    "build.server": "vite build --ssr src/entry.express.tsx",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.types": "tsc --incremental --noEmit",
    "lint": "eslint . --ext .ts,.tsx --no-cache",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "serve": "node server/entry.express.js"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.18.0",
    "@builder.io/qwik-city": "^1.18.0",
    "@builder.io/qwik-react": "^0.5.3",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.7",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@eslint/js": "^9.15.0",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.15.0",
    "typescript-eslint": "^8.0.0",
    "eslint-plugin-qwik": "^1.18.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "undici": "*",
    "vite": "^5.4.2",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "dependencies": {
    "@builder.io/qwik-react": "^0.5.3",
    "@supabase/supabase-js": "^2.57.4",
    "exif-js": "^2.3.0",
    "express": "^4.18.2",
    "heic2any": "^0.0.4",
    "html2canvas": "^1.4.1",
    "i18next": "^23.7.0",
    "jspdf": "^3.0.2",
    "jszip": "^3.10.1",
    "libraw.js": "^3.0.0",
    "lucide-react": "^0.344.0",
    "pdf-lib": "^1.17.1",
    "pdfjs-dist": "^4.8.69",
    "piexifjs": "^1.0.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.8",
    "tesseract.js": "^6.0.1",
    "three": "^0.163.0",
    "tiff.js": "^1.0.0",
    "utif2": "^4.1.0",
    "xlsx": "^0.18.5"
  }
}
